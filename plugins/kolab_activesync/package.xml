		<required>
			<php>
				<min>5.2.1</min>
			</php>
			<pearinstaller>
				<min>1.7.0</min>
			</pearinstaller>
			<package>
				<name>libkolab</name>
				<uri>http://kolabsys.com</uri>
			</package>
			<package>
				<name>jqueryui</name>
				<channel>pear.roundcube.net</uri>
			</package>
		</required>
	</dependencies>
	<phprelease/>
